{
  "name": "Pastel Seaform",
  "type": "dark",
  "colors": {
    "editor.background": "#141f5213",

    "sideBarSectionHeader.background": "#000000",
    "sideBarSectionHeader.foreground": "#ffffff",
    "sideBar.background": "#141f52ea",
    "sideBar.foreground": "#ffffffc2",
    "sideBarTitle.foreground": "#cc00ff",

    "tab.activeBackground": "#1a296d8f",
    "tab.activeForeground": "#ebf77f",
    "tab.hoverBorder": "#ebf77f",
    "tab.inactiveForeground": "#ffffffb0",

    "activityBarBadge.background": "#ebf77f",
    "activityBarBadge.foreground": "#000000",
    "badge.background": "#ebf77f",
    "badge.foreground": "#000000",

    "input.border": "#ffffff70",

    "scrollbarSlider.hoverBackground": "#938a96",

	"statusBar.foreground": "#ffffff",
	"statusBar.background": "#1a296d8f",
	"statusBar.noFolderBackground": "#1a296d8f",

	"activityBar.background": "#1a296dd7",
	"activityBar.foreground": "#ebf77f",
	"activityBar.activeBorder": "#ebf77f",
	
	"titleBar.activeBackground": "#1a296dd7",
	"titleBar.activeForeground": "#ebf77f"
  },
  "tokenColors": [
    {
      "name": "Function",
      "scope": "storage.type.function",
      "settings": {
        "fontStyle": " italic"
      }
    },
    {
      "name": "Function Name",
      "scope": "entity.name.function, meta.method-call.java, text entity.other, meta.object-literal.key",
      "settings": {
        "foreground": "#31a1fd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class definition",
      "scope": "source.java entity",
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#8effc1"
      }
    },
    {
      "name": "Control",
      "scope": "keyword.control, keyword.operator.expression, keyword.other,entity.name",
      "settings": {
        "foreground": "#fcfaaf"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#60945F",
        "fontStyle": " italic   "
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter, variable.other.property",
      "settings": {
        "foreground": "#8effc1"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#ffbeef"
      }
    },
    {
      "name": "Constant Language",
      "scope": "constant.language, constant.numeric, variable.language.this, variable.language.java, source.java storage.modifier, text.xml entity.name.tag.namespace, text markup, text entity.other.attribute-name.namespace",
      "settings": {
        "foreground": "#BD73FF",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Constant User",
      "scope": "source.java constant, variable.other.constant, keyword.control.export.ts",
      "settings": {
        "foreground": "#BD73FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Keys",
      "scope": "meta.object-literal.key string.quoted, support.type.property-name.json",
      "settings": {
        "foreground": "#8effc1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown normal reset",
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#B9B9B9"
      }
    },
    {
      "name": "DOM",
      "scope": "support.function, support.variable.property",
      "settings": {
        "foreground": "#C586E0",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FFC106"
      }
    },
    {
      "name": "Property",
      "scope": "property",
      "settings": {
        "foreground": "#7d6eff"
      }
    },
    {
      "name": "Keyword Operator New",
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#6685FF"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    }
  ]
}
